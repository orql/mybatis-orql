<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.orql.mybatis.mapper.UserMapper">
    <select id="selectByIdFromXml" resultType="com.github.orql.mybatis.schema.User">
        select * from user where id = #{id}
    </select>
</mapper>